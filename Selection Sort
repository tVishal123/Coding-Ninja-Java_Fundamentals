
public class Solution {  

    public static void selectionSort(int[] arr) {
    	//Your code goes here
        for(int i=0; i<arr.length; i++)
        {
            int min = arr[i];
           
            for(int j=i+1; j<arr.length; j++)
            {
                if(arr[i]>arr[j])
                {
                    int t =arr[i];
                   arr[i] = arr[j];
                   arr[j] = t;
                }
            }
           
           
        }
    }   
}
