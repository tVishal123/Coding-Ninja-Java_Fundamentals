public class Solution
{  

  public static int findUnique(int[] arr)
  {
		//Your code goes here
   
    for(int i=0; i< arr.length; i++)
    {
      int t=0;
      for(int j=0; j<arr.length; j++)
      {
        if(arr[i]==arr[j])
        { t++;}
       
      }
      if(t<2)
      {
        return arr[i];
      }
    }
    return 0;
  } 
}
