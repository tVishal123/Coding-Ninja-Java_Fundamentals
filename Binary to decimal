import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        
        
        int d=0;
        int b =1;
        while(n>0)
        {
            int r = n%10;
            
            d =d+r*b;
            b = b*2;
            n =n/10;
        }
        System.out.println(d);
	}
}
