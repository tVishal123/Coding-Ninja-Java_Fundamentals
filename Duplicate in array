import java.util.*;
public class Solution {

	public static int findDuplicate(int[] arr) {
		//Your code goes here
		
		int len = arr.length;
		Arrays.sort(arr);
        for (int i = 0; i <len; i++) {
           
                   if(i<arr.length-1 && arr[i]==arr[i+1])
				   {
					   return arr[i];

				   }
            
        }
		return 0;
        
	}
	
}
