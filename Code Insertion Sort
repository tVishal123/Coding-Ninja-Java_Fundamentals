
public class Solution {  

    public static void insertionSort(int[] arr) {
    	//Your code goes here
       for(int i=1; i<arr.length; i++)
        {
            
           
            int j=i-1;
            int min = arr[i];
            while( j>=0 && arr[j]>min  )
            {
                 arr[j+1] = arr[j];
                j--;
            }
            j++;
            arr[j] = min;
            
        }
    }

}
